try:
          def f(b):
                    r=0
                    for j in range(b):
                              a,c=map(int,input().split())
                              k=abs(a-c)
                              if(k>5):
                                        r=r+1 
                    print(r)
          
          
          for i in range(int(input())):
                    b=int(input())
                    f(b)
          
except:
          pass
